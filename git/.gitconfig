[user]
	name = kang
	email = 1115610574@qq.com
	signingkey =
[color]
	ui = always
[core]
	editor = nvim
    pager = delta
	quotepath = false
[help]
	autocorrect = 11
[interactive]
	diffFilter = delta --color-only
[delta]
    syntax-theme = base16-256
    features = line-numbers
[filter "api_key"]
	smudge = sed \"s/api_key.*/api_key =/\"
	clean = sed \"s/api_key.*/api_key =/\"
[filter "signingkey"]
    clean = sed \"s/\tsigningkey.*/\tsigningkey =/\"
[commit]
	gpgsign = true
[gpg]
	program = gpg
